<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.learn.qingzhi.supergymer">
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".UserSignUp"
            android:label="@string/app_name"
            />
        <activity
            android:name=".UserLogin"
            android:label="@string/app_name"
            />
        <activity android:name=".UserInfoSetup"
            android:label="@string/app_name"
            />
        <activity android:name=".UserHistory"
                  android:label="@string/app_name"/>
        <activity android:name=".Scanner"
                  android:label="@string/app_name"
                  />
        <activity
            android:name=".AndroidSQLiteTutorialActivity"
            android:label="@string/title_activity_android_sqlite_tutorial"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".WelcomePageActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Main_workflow.ExerciseDetailActivity" />
        <activity android:name=".Main_workflow.PartActivity" />

        <activity android:name=".Main_workflow.EquipmentActivity">
        </activity>
    </application>

</manifest>